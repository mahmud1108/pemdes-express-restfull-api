{
    "openapi": "3.0.2",
    "info": {
        "title": "Admin API",
        "version": "1.0"
    },
    "servers": [{ "url": "https://localhost:8000" }],
    "paths": {
        "/api/check_receipts/{no_receipts}": {
            "get": {
                "description": "Guest check receipts",
                "parameters": [
                    {
                        "name": "no_receipts",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Get detail package",
                        "content": {
                            "applicaton/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "no_receipts": {
                                                    "type": "string"
                                                },
                                                "receivers_name": {
                                                    "type": "string"
                                                },
                                                "receivers_phone": {
                                                    "type": "number"
                                                },
                                                "date_address": {
                                                    "type": "string"
                                                },
                                                "current_bumdes": {
                                                    "type": "string"
                                                },
                                                "delivery_status": {
                                                    "type": "string"
                                                },
                                                "acknowledgment": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "package not found",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "not found": {
                                        "description": "Package not found",
                                        "value": {
                                            "errors": {
                                                "message": [
                                                    "package  not found"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/check_cost/{village_id}": {
            "get": {
                "description": "Get shipment cost",
                "parameters": [
                    {
                        "name": "village_id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "name": "weight",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Get detail shipment",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "weight": {
                                                    "type": "number"
                                                },
                                                "cost": {
                                                    "type": "number"
                                                },
                                                "destination": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/admin": {
            "patch": {
                "description": "Update profil admin",
                "responses": {
                    "200": {
                        "description": "Success update",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "email": {
                                            "type": "string"
                                        },
                                        "name": {
                                            "type": "string"
                                        },
                                        "token": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "photo": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "nullable": false
                        }
                    }
                ]
            },
            "get": {
                "description": "Get current admin user",
                "responses": {
                    "200": {
                        "description": "Success get current user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "email": {
                                            "type": "string"
                                        },
                                        "photo": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "description": "Logout admin",
                "responses": {
                    "200": {
                        "description": "Success logout",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/admin/courier": {}
    }
}
